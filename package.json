		"test": "echo \"Error: no test specified\" && exit 1",
		"dev": "ts-node-dev --respawn --transpile-only src/index.ts",
		"build": "tsc",
		"start": "node dist/index.js",
		"migrate:make": "knex migrate:make --knexfile ./knexfile.ts",
		"migrate:latest": "ts-node -r tsconfig-paths/register ./node_modules/.bin/knex migrate:latest --knexfile knexfile.ts",
		"migration": "ts-node -r tsconfig-paths/register ./node_modules/.bin/knex migrate:latest --knexfile ./knexfile.ts",
		"migrate:rollback": "ts-node -r tsconfig-paths/register ./node_modules/.bin/knex migrate:rollback --knexfile ./knexfile.ts",
		"migrate:up": "ts-node -r tsconfig-paths/register ./node_modules/.bin/knex migrate:up --knexfile ./knexfile.ts",
		"migrate:down": "ts-node -r tsconfig-paths/register ./node_modules/.bin/knex migrate:down --knexfile ./knexfile.ts"
	},
